{
	"extends": ["next", "next/core-web-vitals", "eslint:recommended"],
	"plugins": ["prettier"],
	"env": {
		"es6": true
	},
	"globals": {
		"React": "readonly"
	},
	"rules": {
		"no-unused-vars": [
			0,
			{
				"args": "after-used",
				"argsIgnorePattern": "^_"
			}
		],
		"prettier/prettier": "warn"
	}
}
